<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="fair">
	
	<insert id="create" parameterType="fairVO">
	
		insert into fair values (#{mt20id}, #{pfrnm}, #{prfpdfrom}, #{prfpdto}, #{fcltynm}, #{poster}, #{genrenm}, #{prfstate}, #{festival}, #{awards})
	</insert>
	
	<delete id="del"  parameterType="fairVO">
		delete from fair where mt20id = #{id}
	</delete>
	
	<update id="up"  parameterType="fairVO">
		update fair set festival = #{festival} where mt20id = #{id}
	</update>
	
</mapper>











